void fill_arr(int* x, int s) {

	
	int count = 1;
	for (int i = 0; i < s; ++i) {

		if (i == 0) x[i] = 1;
		else x[i] = x[i-1] * (i+1);
	}


}

void fill_v(std::vector<int>& v) {

	
	int count = 1;
	for (int i = 0; i < v.size(); ++i) {

		if (i == 0) v[i] = 1;
		else v[i] = v[i - 1] * (i + 1);
	}


}

int fac(int n) { return n > 1 ? n*fac(n-1) : 1; }


